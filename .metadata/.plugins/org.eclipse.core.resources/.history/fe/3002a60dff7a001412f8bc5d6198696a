package st20024617;
public class Map {
	 public Double array(Double Result, Double longi, Double lat, int maxlongi, int maxlat){
	  
	 double[][] map = new double [3][3];
	
	 //System.out.println(Result +"  "+ longi +"  "+ lat +"  "+maxlongi +"  "+ maxlat);
	 if (longi >= 0 && longi <= maxlongi/3 && lat >= 0 && lat <= maxlat/3){	 
		

		 map[0][0] = map[0][0] + Result;
	 }
	 System.out.println(map[0][0]);
return map[0][0];
	}
}
